.chat-list-header {
    display: flex;
    flex-direction: column;
    padding: 0 0 14px;
    &__searchbar {
        height: 32px;
        background-color: #525252;
        border-radius: 5px;
        border: 0;
        text-align: center;
    }
    &__profile-link {
        align-items: flex-end;
        width: 66px;
        color: #525252;
    }
    &__profile-link-container {
        display: flex;
        justify-content: flex-end;
        padding-bottom: 23px;
    }
}
